set bg_color #f2eeda;

let color = #FF0000;

let shapes = [];
let positions = [];

proc create_circle(position) {
    let circle = Circle{radius: 50, color: color};
    push(positions, position);
    push(shapes, circle);
    draw(position, circle);
}

for i in range(0, 10) {
    create_circle((i * 100, 100));
}